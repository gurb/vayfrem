<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="draftio.DrawingView">
	<Grid x:Name="MainGrid" Background="Aqua">
		<Border x:Name="BorderMain" ClipToBounds="True">
			<Canvas Width="1920" Height="1080" x:Name="Display" Background="White" ZIndex="2" ClipToBounds="True">
				<Canvas.RenderTransform>
					<MatrixTransform/>
				</Canvas.RenderTransform>
			</Canvas>
		</Border>
		<!--<Canvas Width="1920" Height="1080" x:Name="Overlay" Background="Transparent" ZIndex="2">
			<Canvas.RenderTransform>
				<ScaleTransform ScaleX="1" ScaleY="1"/>
			</Canvas.RenderTransform>
		</Canvas>-->

	</Grid>
</UserControl>
